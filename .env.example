# Frontend environment variables for n8n webhook integration

# Required: The n8n webhook that starts generation
VITE_N8N_WEBHOOK_URL="your_n8n_webhook_url_here"

# Optional: Base URL to poll job status; the jobId will be appended
# Example resolves to: `${VITE_N8N_STATUS_URL}/${jobId}`
VITE_N8N_STATUS_URL="your_n8n_status_url_here"
# Optional: Set status method to POST if your status endpoint requires POST
VITE_N8N_STATUS_METHOD="GET"